{
  "name": "codecov-karma-example",
  "version": "1.0.0",
  "description": "An example repository for showing how to generate the codecov reports with Karma testing framework.",
  "main": "src.js",
  "scripts": {
    "test": "karma start karma.conf.js",
    "ci": "karma start karma.conf.js --browsers PhantomJS",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "keywords": [
    "karma",
    "codecov"
  ],
  "author": "Yoshiya Hinosawa <stibium121@gmail.com> (https://twitter.com/kt3k)",
  "repository": "git@github.com:kt3k/codecov-karma-example.git",
  "license": "Public Domain",
  "peerDependencies": {
    "jquery": "^2.1.4"
  },
  "devDependencies": {
    "chai": "^3.3.0",
    "codecov.io": "^0.1.6",
    "jquery": "^3.1.1",
    "karma": "^1.4.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "mocha": "^3.2.0",
    "phantomjs": "^2.1.7"
  }
}
